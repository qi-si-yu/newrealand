TNZ=window.TNZ||{},TNZ.BusinessFinder=function(i,r){if(i=i||{},r=r||jQuery,!1==!!i.block_unique_id||0===i.block_unique_id.length)throw new Error('Settings param "block_unique_id" is required. This is the div containing the finder.');r("#"+i.block_unique_id).find("form").on("submit",function(i){i.preventDefault();var n=[],e=r(this);e.find("select[name='filters'] option:selected").each(function(){var i=r(this).val();"-1"!==i&&n.push(i)});var t=e.attr("action");if(n.length){var o=-1==t.indexOf("?")?"?":"&";t+=o+"filters="+n.join(",")}window.location.href=t})};