TNZ=window.TNZ||{},TNZ.GettingHere=function(e){if((e=e||{}).isincountry=typeof e.isincountry==typeof!0==!0?e.isincountry:null,!1==!!e.block_unique_id)throw new Error('Settings param "block_unique_id" is required.');if(null==e.isincountry)throw new Error('Settings param "isincountry" is required. and MUST be a boolean true|false value');var t={elBlock:document.getElementById(e.block_unique_id)};return t.elBlock&&(t.elHeadText=t.elBlock.querySelectorAll(".js-getting-here-text-out"),t.elImgInCountry=t.elBlock.querySelectorAll(".js-getting-here-img-in"),t.elImgOutCountry=t.elBlock.querySelectorAll(".js-getting-here-img-out"),e.isincountry?t.elImgInCountry.length&&t.elImgInCountry[0].removeAttribute("hidden"):(t.elHeadText.length&&t.elHeadText[0].removeAttribute("hidden"),t.elImgOutCountry.length&&t.elImgOutCountry[0].removeAttribute("hidden")),TNZ.Tools.triggerResizeEnd()),{}};