TNZ=window.TNZ||{},TNZ.Links=function(i){i=i||jQuery;function a(n,t){var e=n.data(t),i="";return""!=e&&null!=e&&(i=' class="'+e+'"'),i}function n(){i("body").on("touchstart click",".js-display-link:not(:disabled)",function(n){"click"==n.type&&(n.preventDefault(),function(n){var t=n.find(".js-icon").clone(),e=a(n,"phoneclass");n.after("<span"+e+">"+i("<div>").append(t).html()+function(n){return'<span class="o-phone__number">'+n+"</span>"}(n.attr("href"))+"</span>"),n.attr("hidden",!0)}(i(this)))})}(function(){i("body").on("touchstart click",".js-phone-link:not(:disabled)",function(n){TNZ.Device.isMediaQuery(">phablet")&&"click"==n.type&&(n.preventDefault(),function(n){var t=n.find(".js-icon").clone(),e=a(n,"phoneclass");n.after("<span"+e+">"+i("<div>").append(t).html()+function(n){return'<span itemprop="telephone" class="o-phone__number">'+n+"</span>"}(function(n){return n.replace("tel:","")}(n.attr("href")))+"</span>"),n.attr("hidden",!0)}(i(this)))})})(),n()},TNZ.JSLib.requireLoadEvent(["jqready"],function(){TNZ.Links(jQuery)});